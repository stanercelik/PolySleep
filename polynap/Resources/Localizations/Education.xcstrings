    "education.article.adaptationProcess.summary" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Understanding the challenging but critical transition period when your body adapts to polyphasic sleep."
          }
        },
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Vücudunuzun polifazik uykuya adapte olduğu zorlu ama kritik geçiş dönemini anlama."
          }
        }
