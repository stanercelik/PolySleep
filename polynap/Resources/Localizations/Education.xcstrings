    },
    "education.article.adaptationChallenges.summary" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Understanding the symptoms and challenges you'll face during the adaptation process."
          }
        },
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Adaptasyon sürecinde karşılaşacağınız semptomları ve zorlukları anlama."
          }
        }
