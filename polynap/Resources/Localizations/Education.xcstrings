          "stringUnit" : {
            "state" : "translated",
            "value" : "The primary promise of polyphasic sleep: gaining 3-6 extra hours of wakefulness daily."
          }
        },
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Polifazik uykunun temel vaadi: günde 3-6 saat ekstra uyanıklık kazanma."
          }
        }
