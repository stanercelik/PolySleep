    "education.article.benefits.mentalClarity.summary" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Reports of continuous mental clarity and focus throughout the day after successful adaptation."
          }
        },
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Başarılı adaptasyon sonrası gün boyunca sürekli zihinsel netlik ve odaklanma raporları."
          }
        }
