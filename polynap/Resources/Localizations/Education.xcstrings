    "education.article.risks.mentalHealth.summary" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Chronic sleep deprivation significantly increases the risk of anxiety, depression, and other mental health disorders."
          }
        },
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Kronik uyku yoksunluğu anksiyete, depresyon ve diğer zihinsel sağlık bozuklukları riskini önemli ölçüde artırır."
