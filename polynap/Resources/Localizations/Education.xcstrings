    "education.article.myth.intelligence.summary" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Debunking the myth that famous historical figures practiced polyphasic sleep."
          }
        },
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Ünlü tarihsel figürlerin polifazik uyku uyguladığı efsanesini çürütme."
