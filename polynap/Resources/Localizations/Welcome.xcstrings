    "thirdPageDesc" : {
      "comment" : "Description text of third welcome page",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Polyphasischer Schlaf teilt deine Ruhe in effiziente Zyklen auf und gibt deinem Gehirn die Energie, die es wirklich braucht."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Polyphasic sleep divides your rest into efficient cycles, giving your brain the energy it truly needs."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "多相睡眠は、休息を効率的なサイクルに分割して、脳が本当に必要とするエネルギーを与えてくれるんだ。"
          }
        },
        "ms" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Tidur polifasa membahagikan rehat anda kepada kitaran yang cekap, memberikan otak anda tenaga yang benar-benar diperlukannya."
