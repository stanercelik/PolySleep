    "firstPageDesc" : {
      "comment" : "Description text of first welcome page",
      "localizations" : {
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Mit deinem Schlaf zu beginnen ist der beste Weg! Indem du auf polyphasischen Schlaf umsteigst, kannst du eine produktivere und energiegeladenere Version von dir freischalten."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Starting with your sleep is the best way to do it! By switching to polyphasic sleep, you can unlock a more productive and energetic version of yourself."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "まずは睡眠から始めるのが一番！多相睡眠に切り替えて、もっと生産的でエネルギッシュな自分を解放しよう。"
          }
        },
        "ms" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Bermula dengan tidur anda adalah cara terbaik untuk melakukannya! Dengan beralih kepada tidur polifasa, anda boleh membuka kunci versi diri anda yang lebih produktif dan bertenaga."
          }
        },
        "th" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "การเริ่มต้นกับการนอนของคุณคือวิธีที่ดีที่สุด! ด้วยการเปลี่ยนไปใช้การนอนหลับแบบหลายเฟส, คุณสามารถปลดล็อกเวอร์ชันที่มีประสิทธิผลและมีพลังมากขึ้นของตัวคุณเองได้"
          }
        },
        "tr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Uykudan başlamak bunun için en iyi yol! Çok fazlı uykuya geçerek kendinin daha üretken ve enerjik bir versiyonunu oluşturabilirsin."
          }
        }
      }
